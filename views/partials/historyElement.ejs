<p> 
  <img src="https://ddragon.leagueoflegends.com/cdn/<%= version %>/img/champion/<%= match.championName %>.png" 
  width="50px" height="50px">

<% if(match.win) { %>
  <span style="color:green;font-weight:bold">Victoire ! -</span><span>&nbsp; Score : <%= match.k %>/<%= match.d %>/<%= match.a %> - valeur : <span style="color:green;font-weight:bold">+<%= match.lpValue %></span> date : <span id="dateGame<%= match.timestamp %>"><%= new Date(match.timestamp).toLocaleString() %></span></span>
<% } else {%>
  <span style="color:red">DÃ©faite ! -</span><span>&nbsp; Score : <%= match.k %>/<%= match.d %>/<%= match.a %> - valeur : <span style="color:red"><%= match.lpValue == 0 ? "-0" : match.lpValue %></span> date : <span id="dateGame<%= match.timestamp %>"><%= new Date(match.timestamp).toLocaleString() %></span></span>
  <% } %>

  <script type="text/javascript">
    dateGame = new Date(<%= match.timestamp %>)
    document.getElementById('dateGame<%= match.timestamp %>').innerHTML = dateGame.toLocaleString();

  </script>
</p>
